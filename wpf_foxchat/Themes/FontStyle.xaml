<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:wpf_foxchat.Themes">
    <!--Font family use application-->
    <FontFamily x:Key="FontAwesome">/Fonts/FontAwesome.ttf#Font Awesome 6 Free Solid</FontFamily>
    <FontFamily x:Key="FontRobotoMono">RobotoMono</FontFamily>
    <!--Define color text-->
    <SolidColorBrush  x:Key="TextColor">#777879</SolidColorBrush>
    <SolidColorBrush  x:Key="TextHoverColor">#0F82FF</SolidColorBrush>
    <SolidColorBrush  x:Key="TextHighLightColor">#BABCC0</SolidColorBrush>
</ResourceDictionary>